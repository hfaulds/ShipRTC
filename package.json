{
  "name": "ShipRTC",
  "version": "0.0.1",
  "description": "An experiment using RTCPeerConnection to create server-less multiplayer games",
  "repository":  "https://github.com/hfaulds/ShipRTC",
  "dependencies": {
    "express": "4.12.3",
    "socket.io": "1.3.5",
    "socket.io-client": "1.3.5",
    "lodash": "3.6.0",
    "machina": "1.0.0",
    "request": "2.54.0",
    "pixi.js": "2.2.7",
    "react-pixi": "0.4.0",
    "react": "0.12.2",
    "reactify": "1.1.0",
    "node-jsx": "0.12.4",
    "alt": "0.14.5",
    "browserify": "latest"
  },
  "devDependencies": {
    "jasmine-core": "^2.2.0",
    "karma": "~0.12.31",
    "karma-jasmine": "^0.3.5",
    "karma-chrome-launcher": "~0.1",
    "karma-browserify": "4.1.2",
    "watchify": "latest"
  },
  "scripts": {
    "postinstall": "browserify -t reactify js/client_main.js > public/js/bundle.js",
    "watch-js": "watchify --debug -t reactify js/client_main.js -o public/js/bundle.js"
  },
  "engines": {
    "node": "0.12.x"
  }
